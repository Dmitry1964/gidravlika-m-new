mixin kmu-tractor-list()
  -
    const kmus = [
      {
        path: "kmu",
        image: "dvina-32009",
        format: "jpg",
        width: 218,
        height: 160,
        name: "Кран-манипулятор",
        title: "Двина 3200.9",
        base: "МТЗ-92П",
        liftingMin: "400",
        liftingMax: "3200",
        boomMin: "1.8",
        boomMax: "9",
        rotation: "360°",
        href: "#",
        altText: "Кран манипулятор",
      },
    ]
  section.section-list
    .container
      .section-list__wrapper
        h2.visually-hidden Автовышки
        +aside-menu-vipo()
        ul.catalog__list.catalog__list--vipo.catalog__list--start
          each item in kmus
            li.catalog__item
              .catalog__item-image
                +picture(item.path, item.image, item.format, item.width, item.height, item.altText)
                span=item.name
                h2.catalog__item-title=item.title
              .catalog__item-params
                dl
                  dt Модель шасси
                  dd=item.base
                  dt Вылет min, м
                  dd=item.boomMin
                  dt Вылет max, м
                  dd=item.boomMax
                  dt Грузоподъемность
                  dd=""
                  dt минимальный вылет, кг
                  dd=item.liftingMax
                  dt максимальный вылет, кг
                  dd=item.liftingMin
                  dt Угол поворота стрелы
                  dd=item.rotation
              +btn({text: "Подробнее", href: item.href})
