  section.mksm-unit
    .mksm-unit__title
      .container
        h2 Минипогрузчик Четра МКСМ-1200М
        p Комплектация "Премиум"
    .container
      .mksm-unit__content
        .mksm-unit__images.swiper
          .mksm-init__list.swiper-wrapper
            for item in images
              .mksm-unit__item.swiper-slide
                +picture(item.path, item.image, item.format, item.width, item.height, item.altText)
          .swiper-button-next(style={color: '#f3b411'})
          .swiper-button-prev(style={color: '#f3b411'})
          .swiper-pagination
        .mksm-unit__dimensions
          +picture( "mksm1200mp", "mksm1200mp-graf", "jpg", 417, 590, "Габаритные размеры")
        .mksm-unit__interiors.swiper
          h2 Кабина и навесное оборудование
          .mksm-init__interiors-list.swiper-wrapper
            for item in interiors
              .mksm-unit__interiors-item.swiper-slide
                +picture(item.path, item.image, item.format, item.width, item.height, item.altText)
          .swiper-button-next(style={color: '#f3b411'})
          .swiper-button-prev(style={color: '#f3b411'})
          .swiper-pagination
        .mksm-unit__info
